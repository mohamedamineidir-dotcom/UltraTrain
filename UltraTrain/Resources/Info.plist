<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CFBundleDevelopmentRegion</key>
    <string>en</string>
    <key>CFBundleDisplayName</key>
    <string>UltraTrain</string>
    <key>CFBundleExecutable</key>
    <string>$(EXECUTABLE_NAME)</string>
    <key>CFBundleIdentifier</key>
    <string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
    <key>CFBundleInfoDictionaryVersion</key>
    <string>6.0</string>
    <key>CFBundleName</key>
    <string>$(PRODUCT_NAME)</string>
    <key>CFBundlePackageType</key>
    <string>APPL</string>
    <key>CFBundleShortVersionString</key>
    <string>$(MARKETING_VERSION)</string>
    <key>CFBundleVersion</key>
    <string>$(CURRENT_PROJECT_VERSION)</string>
    <key>LSRequiresIPhoneOS</key>
    <true/>
    <key>UIApplicationSceneManifest</key>
    <dict>
        <key>UIApplicationSupportsMultipleScenes</key>
        <false/>
    </dict>
    <key>UILaunchScreen</key>
    <dict>
        <key>UIColorName</key>
        <string>LaunchBackground</string>
        <key>UIImageName</key>
        <string>LaunchIcon</string>
    </dict>
    <key>UISupportedInterfaceOrientations</key>
    <array>
        <string>UIInterfaceOrientationPortrait</string>
    </array>
    <key>UIBackgroundModes</key>
    <array>
        <string>location</string>
        <string>remote-notification</string>
        <string>fetch</string>
        <string>processing</string>
    </array>
    <key>BGTaskSchedulerPermittedIdentifiers</key>
    <array>
        <string>com.ultratrain.app.healthkit-sync</string>
        <string>com.ultratrain.app.recovery-calc</string>
        <string>com.ultratrain.app.sync-queue</string>
    </array>
    <key>NSLocationWhenInUseUsageDescription</key>
    <string>UltraTrain uses your location to track your runs and display your route on the map.</string>
    <key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
    <string>UltraTrain uses background location to continue tracking your run when the app is in the background.</string>
    <key>NSHealthShareUsageDescription</key>
    <string>UltraTrain reads your heart rate, workout, and activity data to track your training progress and fitness.</string>
    <key>NSHealthUpdateUsageDescription</key>
    <string>UltraTrain saves your completed runs as workouts in Apple Health.</string>
    <key>NSCameraUsageDescription</key>
    <string>UltraTrain uses the camera to scan food product barcodes for quick nutrition entry.</string>
    <key>NSFaceIDUsageDescription</key>
    <string>UltraTrain uses Face ID to protect your training and health data.</string>
    <key>NSUserTrackingUsageDescription</key>
    <string>UltraTrain uses this permission to measure app performance and improve your training experience. No personal data is shared with third parties.</string>
    <key>NSSupportsLiveActivities</key>
    <true/>
    <key>STRAVA_CLIENT_ID</key>
    <string>$(STRAVA_CLIENT_ID)</string>
    <key>STRAVA_CLIENT_SECRET</key>
    <string>$(STRAVA_CLIENT_SECRET)</string>
    <key>HMAC_SIGNING_SECRET</key>
    <string>$(HMAC_SIGNING_SECRET)</string>
    <key>CFBundleURLTypes</key>
    <array>
        <dict>
            <key>CFBundleURLName</key>
            <string>com.ultratrain.strava</string>
            <key>CFBundleURLSchemes</key>
            <array>
                <string>ultratrain</string>
            </array>
        </dict>
    </array>
</dict>
</plist>
